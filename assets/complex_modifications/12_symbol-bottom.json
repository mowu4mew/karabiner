{
  "title":"12/13.Symbol-bottom",
  "rules":[
		{
			"description":"tilda/quote/[/(/)/]/semicolon/]/back slash from Kana + Z/X/C/V/B/N/M/comma/period/slash to tilda/quote/[/(/)/]/semicolon/]/back slash",
			"manipulators":[
				{
					"type":"basic",
					"from":{
						"key_code":"z",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock"]
						}
					},
					"to":[
						{
							"key_code":"equal_sign",
							"modifiers":"shift"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"x",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock"]
						}
					},
					"to":[
						{
							"key_code":"7",
							"modifiers":"shift"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"c",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock"]
						}
					},
					"to":[
						{
							"key_code":"close_bracket"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"v",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock"]
						}
					},
					"to":[
							{"key_code":"vk_none"}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"b",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock","shift"]
						}
					},
					"to":[
						{
							"key_code":"8",
							"modifiers":"shift"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"m",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["any"]
						}
					},
					"to":[
						{
							"key_code":"vk_none"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"n",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["any"]
						}
					},
					"to":[
						{
							"key_code":"9",
							"modifiers":"shift"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"comma",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock"]
						}
					},
					"to":[
						{
							"key_code":"backslash"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"period",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock","shift"]
						}
					},
					"to":[
						{
							"key_code":"semicolon"
						}
					]
				},
				{
					"type":"basic",
					"from":{
						"key_code":"slash",
						"modifiers":{
							"mandatory":["right_option"],
							"optional":["caps_lock","shift"]
						}
					},
					"to":[
						{
							"key_code":"international3"
						}
					]
				}
			]
    }
 ]
